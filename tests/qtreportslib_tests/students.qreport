<?xml version="1.0" encoding="UTF-8"?>
<report name="sample_report">
  <style name="Arial_Normal" isDefault="true" fontName="Arial"
    fontSize="12" pdfFontName="c:\tahoma.ttf" pdfEncoding="Cp1251"
    isPdfEmbedded="false" />

  <queryString>
    <![CDATA[ select stud_name, group_name, dep_name from students_t, groups_t, departments_t where students_t.group_id = groups_t.group_id and groups_t.dep_id = departments_t.dep_id; ]]>
  </queryString>

  <field name="stud_name" class="QString" />
  <field name="group_name" class="QString" />
  <field name="dep_name" class="QString" />

  <title>
    <!--<band height="40">
      <textField>
        <reportElement x="50" y="10" width="450" height="30" />
        <textFieldExpression class="QString">
          <![CDATA[Название отчета: $P{title}]]>
        </textFieldExpression>
      </textField>
    </band>-->
    <band height="50">
      <staticText>
        <reportElement x="10" y="10" width="150" height="30" />
        <text><![CDATA[Список студентов: ]]></text>
      </staticText>
    </band>
  </title>
  <detail>
    <band height="40">
      <textField>
        <reportElement x="5" y="5" width="180" height="30" />
        <textFieldExpression class="QString">
          <![CDATA[$F{stud_name}]]>
        </textFieldExpression>
      </textField>
      <textField>
        <reportElement x="190" y="5" width="200" height="30" />
        <textFieldExpression class="QString">
          <![CDATA[$F{group_name}]]>
        </textFieldExpression>
      </textField>
      <textField>
        <reportElement x="405" y="5" width="190" height="30" />
        <textFieldExpression class="QString">
          <![CDATA[$F{dep_name}]]>
        </textFieldExpression>
      </textField>
    </band>
  </detail>

</report>
