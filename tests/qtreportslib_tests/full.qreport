<?xml version="1.0" encoding="UTF-8"?>
<report name="full_report" pageWidth = "500" pageHeight = "600" orientation = "Landscape">
  
<style name="Arial_Normal" isDefault="true" fontName="Arial"
    fontSize="12" pdfFontName="c:\tahoma.ttf" pdfEncoding="Cp1251"
    isPdfEmbedded="false" isBold = "true" isItalic = "true" fontColor = "red"/>

  <queryString>
    <![CDATA[ select idImg, nameImg, image from images; ]]>
  </queryString>

  <field name="idImg" class="integer" />
  <field name="nameImg" class="QString" />
  <field name="image" class="QString" />

  <title>
    <band height = "200">
      <staticText>
  	<reportElement x="0" y="40" width="200" height="20"/>
  	<textElement textVAlignment="Top" textAlignment="Right"/>
  	<text><![CDATA[TopRight Text]]></text>
      </staticText>
    </band>
  </title>

  <detail>
    <band height="200">
      <textField>
        <reportElement x="20" y="20" width="100" height="30" />
        <textFieldExpression class="QString"><![CDATA[$P{param1}]]></textFieldExpression>
      </textField>
    </band>
    <band height="230">
      <textField>
        <reportElement x="0" y="0" width="50" height="20" />
        <textFieldExpression class="QString"><![CDATA[$F{idImg}]]></textFieldExpression>
      </textField>
      <textField>
        <reportElement x="51" y="0" width="50" height="20" />
	<textElement textVAlignment="Bottom" textAlignment="Left"/>
        <textFieldExpression class="QString">
          <![CDATA[$F{nameImg}]]>
        </textFieldExpression>
      </textField>
      <image scaleImage="Clip" onErrorType="Icon" isLazy="true">
        <reportElement x="102" y="0" width="425" height="200" />
        <imageExpression class="QString"><![CDATA[$F{image}]]></imageExpression>
      </image>
    </band>
    <band height = "100">
      <rect>
        <reportElement x="50" y="0" width="100" height="100" />
      </rect>
      <ellipse>
        <reportElement x="50" y="0" width="100" height="100" />
      </ellipse>
      <line>
        <reportElement x="100" y="0" width="1" height="100" />
      </line>
    </band>
  </detail>

</report>
